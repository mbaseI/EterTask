{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "react/jsx-uses-vars": "warn",
    "react/jsx-uses-react": "warn",
    "no-unused-vars": "warn",
    "no-console": "warn"
  }
}
